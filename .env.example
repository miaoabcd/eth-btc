# Environment overrides (optional)
# - STRATEGY_* overrides config.toml values
# - HYPERLIQUID_* / STRATEGY_PRIVATE_KEY provide auth overrides
# - Prefer config.toml for a single-source setup
# - CLI overrides available: --state-path, --interval-secs, --once, --paper, --disable-funding, --base-url

# Runtime logging (tracing subscriber)
RUST_LOG=info

# Strategy overrides (STRATEGY_* prefix)
STRATEGY_ENTRY_Z=1.5
STRATEGY_TP_Z=0.45
STRATEGY_SL_Z=3.5
STRATEGY_N_Z=384

# Sigma floor
STRATEGY_SIGMA_FLOOR_MODE=CONST
STRATEGY_SIGMA_FLOOR_CONST=0.001
STRATEGY_SIGMA_FLOOR_QUANTILE_WINDOW=30
STRATEGY_SIGMA_FLOOR_QUANTILE_P=0.1
STRATEGY_EWMA_HALF_LIFE=20

# Position sizing
STRATEGY_C_MODE=FIXED_NOTIONAL
STRATEGY_C_VALUE=50000
STRATEGY_EQUITY_VALUE=100000
STRATEGY_EQUITY_RATIO_K=0.1
STRATEGY_MAX_NOTIONAL=100000
STRATEGY_N_VOL=672
STRATEGY_MAX_POSITION_GROUPS=1

# Funding controls
STRATEGY_FUNDING_MODE=FILTER
STRATEGY_FUNDING_COST_THRESHOLD=0.001
STRATEGY_FUNDING_THRESHOLD_K=0.5
STRATEGY_FUNDING_SIZE_ALPHA=0.5
STRATEGY_C_MIN_RATIO=0.3

# Risk controls
STRATEGY_MAX_HOLD_HOURS=48
STRATEGY_COOLDOWN_HOURS=24
STRATEGY_CONFIRM_BARS_TP=0

# Data/Execution
STRATEGY_PRICE_FIELD=MID
STRATEGY_ORDER_TYPE=MARKET
STRATEGY_SLIPPAGE_BPS=5

# Logging/Alerts
STRATEGY_LOG_LEVEL=info
STRATEGY_LOG_FORMAT=json
STRATEGY_ALERT_WEBHOOK_URL=
STRATEGY_ALERT_EMAIL_ENABLED=false

# Live trading auth (required for live; optional for paper/backtest)
# Private key can be provided via any of these (first match wins):
HYPERLIQUID_PRIVATE_KEY=
STRATEGY_PRIVATE_KEY=
STRATEGY_API_KEY=
# Optional vault address (use one of these)
HYPERLIQUID_VAULT_ADDRESS=
HYPERLIQUID_VAULT=

# Backtest
STRATEGY_BACKTEST_INCLUDE_FEES=true
STRATEGY_BACKTEST_FEE_BPS=2
STRATEGY_BACKTEST_INCLUDE_SLIPPAGE=true
STRATEGY_BACKTEST_SLIPPAGE_BPS=5
STRATEGY_BACKTEST_INCLUDE_FUNDING=true
